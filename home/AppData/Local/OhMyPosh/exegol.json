{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "cyan",
          "properties": {
            "template": "[{{ .Icon }} {{ .PWD }}] "
          },
          "style": "powerline",
          "type": "os"
        },
        {
          "properties": {
            "template": "<cyan>[</><lightGreen>{{ .HEAD }}</><cyan>]</> "
          },
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#C94A16",
          "style": "powerline",
          "type": "exit"
        },
        {
          "foreground": "white",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}
